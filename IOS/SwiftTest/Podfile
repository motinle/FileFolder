#flutter_iphone=1 pod update --no-repo-update

source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'

platform :ios, '10.0'

$isArchive = false
$isFlutterDevelop = true


target 'SwiftTest' do
  #pod 'AFNetworking', '3.2.1'
  
  if $isArchive == true#打正式包
    pod 'FlutterSDK', :git => 'https://github.com/motinle/FlutterSDK.git', :branch=>'master'
  else
    if $isFlutterDevelop == true
      if ENV['flutter_iphone']#本地真机库
        pod 'flutter_iphone', :path => '../../Flutter/my_flutter'
      else#本地摸拟器库
        pod 'flutter_simulator', :path => '../../Flutter/my_flutter'
      end
    end
  end
  
  
end

